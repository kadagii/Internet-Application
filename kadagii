#include <iostream>

int main() {
    int scores[5] = {10, 20, 15, 25, 30}; // Initializing an array with five integers representing scores

    int total = 0; // Variable to store the total sum of scores

    // Calculating the sum of scores
    for(int i = 0; i < 5; i++) {
        total += scores[i];
    }

    // Displaying the total sum of scores
    std::cout << "Total points scored: " << total << std::endl;

    return 0;
}
